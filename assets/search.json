---
layout: none
---

[
  {% for post in collections["posts"] %}
    {
      "title"    : "{{ post.document.frontmatter.title | escape }}",
      "tags"     : "{{ post.document.frontmatter.tags | join: ', ' }}",
      "url"      : "{{ global.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
