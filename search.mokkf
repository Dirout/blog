---
permalink: "/assets/search.json"
markdown: false
---
[
  {% for post in collections["posts"] %}
    {
      "title"    : "{{ post.data.title | escape }}",
      "tags"     : "{{ post.data.tags | join: ', ' }}",
      "url"      : "{{ global.url }}{{ post.url }}",
      "date"     : "{{ post.date.rfc_3339 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
